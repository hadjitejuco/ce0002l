//create an array -> display the numbers in reverse order
#include <stdio.h>
int main(){
	int n, i, a[100];
	printf ("enter n: ");
	scanf ("%d",&n);
	printf ("Enter %d numbers ",n);
	//input
	for (i=0;i<n;i++){
		scanf ("%d",&a[i]);
	}
	printf ("Entered numbers in reverse order \n");
	for (i=n-1;i>=0;i--)
	{
		printf ("%d \t",a[i]);
	}	
	return 0;
}
