//two dim
//a[10][10];
#include <stdio.h>
int main(){
	int n,m,i,j,a[10][10];
	printf ("Enter order of matrix");
	scanf ("%d%d",&n,&m);
	for (i=1; i<=n;i++)
		for (j=1; j<=m;j++)
			scanf ("%d",&a[i][j]);
	printf ("\nGiven Matrix\n");
	for (i=1; i<=n;i++){
		for (j=1; j<=m;j++)
			printf ("%8d",a[i][j]);
			printf ("\n");
	}
	return 0;
}

