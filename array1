#include <stdio.h>
int  main(){
	int a,b,c,d,e;
	printf ("Enter 1st number:\n");
	scanf ("%d",&a);
	printf ("Enter 2nd number:\n");
	scanf ("%d",&b);
	printf ("Enter 3rd number:\n");
	scanf ("%d",&c);
	printf ("Enter 4th number:\n");
	scanf ("%d",&d);
	printf ("Enter 5th number:\n");
	scanf ("%d",&e);
	printf ("Value of 1st number is %d\n",a);
	printf ("Value of 2nd number is %d\n",b);
	printf ("Value of 3rd number is %d\n",c);
	printf ("Value of 4th number is %d\n",d);
	printf ("Value of 5th number is %d\n",e);
	return 0;
}
